#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hamidev/dev/RestoGest/node_modules/.pnpm/ts-dev@2.1.16/node_modules/ts-dev/dist/esm/node_modules:/home/hamidev/dev/RestoGest/node_modules/.pnpm/ts-dev@2.1.16/node_modules/ts-dev/dist/node_modules:/home/hamidev/dev/RestoGest/node_modules/.pnpm/ts-dev@2.1.16/node_modules/ts-dev/node_modules:/home/hamidev/dev/RestoGest/node_modules/.pnpm/ts-dev@2.1.16/node_modules:/home/hamidev/dev/RestoGest/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/hamidev/dev/RestoGest/node_modules/.pnpm/ts-dev@2.1.16/node_modules/ts-dev/dist/esm/node_modules:/home/hamidev/dev/RestoGest/node_modules/.pnpm/ts-dev@2.1.16/node_modules/ts-dev/dist/node_modules:/home/hamidev/dev/RestoGest/node_modules/.pnpm/ts-dev@2.1.16/node_modules/ts-dev/node_modules:/home/hamidev/dev/RestoGest/node_modules/.pnpm/ts-dev@2.1.16/node_modules:/home/hamidev/dev/RestoGest/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  --unhandled-rejections=strict --enable-source-maps "$basedir/../.pnpm/ts-dev@2.1.16/node_modules/ts-dev/dist/esm/ts-dev.js" "$@"
else
  exec node  --unhandled-rejections=strict --enable-source-maps "$basedir/../.pnpm/ts-dev@2.1.16/node_modules/ts-dev/dist/esm/ts-dev.js" "$@"
fi
